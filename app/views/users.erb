<h1> New user </h1>
<% if flash[:error] %>
<div>
<%= flash[:error] %>
</div>
<% end %>
<form action='/users/new' method='POST'>
  <input type ='text' name='email' />
  <input type ='password' name='password' />
  <input type ='password' name='password_confirmation' />
  <input type='submit' value='Sign up' />
</form>
